{
	"version": 1.0,
	"binds": {
		"New": ["Ctrl-Shift-N", "New"],
		"Open": ["Ctrl-O"],
		"Save": ["Ctrl-S"],

		"Undo": ["Ctrl-Z", "Undo"],
		"Redo": ["Ctrl-Shift-Z", "Redo"],

		"Cut": ["Ctrl-X", "Cut"],
		"Copy": ["Ctrl-C", "Copy"],
		"Paste": ["Ctrl-V", "Paste"],

		"Find": ["Ctrl-F"],
		"Replace": ["Ctrl-H"],

		"Help": ["F1"]
	}
}

