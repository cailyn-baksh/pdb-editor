{
	"version": 1.0,
	"binds": {
		"new": ["Ctrl-Shift-N", "New"],
		"open": ["Ctrl-O"],
		"save": ["Ctrl-S"],

		"undo": ["Ctrl-Z", "Undo"],
		"redo": ["Ctrl-Shift-Z", "Redo"],

		"cut": ["Ctrl-X", "Cut"],
		"copy": ["Ctrl-C", "Copy"],
		"paste": ["Ctrl-V", "Paste"],

		"find": ["Ctrl-F"],
		"replace": ["Ctrl-H"],

		"help": ["F1"]
	}
}

